<view class="container">
	<view class="title-container">
		<view class="title">招商1+N供应链项目</view>
		<view class="explain">
			<view>用户列表:</view>
		</view>
	</view>

	<view class="table">
		<view class="tr bg-w">
			<view class="th">手机号</view>
			<view class="th">处理阶段</view>
			<view class="th ">处理时间</view>
      <view class="th ">详情</view>
		</view>
		<block wx:for="{{listData}}" wx:key="{[code]}">
			<view class="tr bg-g" wx:if="{{index % 2 == 0}}">
				<view class="td">{{item.phone}}</view>
				<view class="td">{{item.steps}}</view>
				<view class="td">{{item.time}}</view>
        <view class="td"> 
        <view  bindtap="onCheck" class="s-Style">查看详情</view>
        </view>
			</view>
			<view class="tr" wx:else>
					<view class="td">{{item.phone}}</view>
				<view class="td">{{item.steps}}</view>
				<view class="td">{{item.time}}</view>
        <view class="td"> 
        <view  bindtap="onCheck" class="s-Style">查看详情</view>
        </view>
			</view>
		</block>
	</view>
</view>